{
    "name": "kjudge-frontend",
    "version": "0.7.0",
    "description": "Templates and others for kjudge.",
    "repository": "https://github.com/natsukagami/kjudge",
    "author": "Natsu Kagami",
    "license": "AGPL-3.0",
    "private": true,
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^3.1.3",
        "@types/humanize-duration": "^3.18.0",
        "@types/node": "^14.14.14",
        "@types/react": "^17.0.0",
        "autoprefixer": "^9.7.5",
        "parcel": "^2.0.0",
        "prettier": "2.2.1",
        "tailwindcss": "^2.0.0",
        "typescript": "^4.1.3"
    },
    "scripts": {
        "test": "echo No tests... yet",
        "fmt": "prettier \"css/**/*\" \"ts/**/*\" \"*.json\" \"*.js\"",
        "dev": "parcel watch --dist-dir ../templates \"html/**/*.html\"",
        "build": "DEST_DIR=\"../templates\" yarn build:to",
        "build:to": "mkdir -p \"$DEST_DIR\" && rm -rf \"$DEST_DIR\"/* && parcel build --no-source-maps --dist-dir \"$DEST_DIR\" \"html/**/*.html\""
    },
    "alias": {
        "react": "preact/compat",
        "react-dom/test-utils": "preact/test-utils",
        "react-dom": "preact/compat",
        "react/jsx-runtime": "preact/jsx-runtime",
        "preact/jsx-dev-runtime": "preact/jsx-runtime"
    },
    "dependencies": {
        "fontsource-ibm-plex-mono": "^3.1.5",
        "fontsource-muli": "^3.1.5",
        "highlight.js": "^10.4.1",
        "humanize-duration": "^3.22.0",
        "preact": "10.6.*",
        "react-flip-move": "^3.0.4",
        "regenerator-runtime": "^0.13.5"
    }
}
